  "private": true,
  "homepage": ".",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "gh-pages": "^2.0.1",
    "inliner": "^1.13.1",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.8",
    "react-dom": "^16.8.6",
    "react-json-pretty": "^2.1.0",
    "react-scripts": "2.1.8",
